{
  "name": "peof-agile",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "validate": "node ./node_modules/dependency-cruiser/bin/dependency-cruise --validate .arch-fitness.json src --ts-pre-compilation-deps",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "heroku-postbuild": "npm run build",
    "inspect": "node --inspect src/app.ts",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "winston": "^3.2.1",
    "node-cron": "^2.0.3",
    "request": "^2.88.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.11.7",
    "@types/node-cron": "^2.0.2",
    "@types/request": "^2.48.3",
    "dependency-cruiser": "^5.4.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "@types/shortid": "^0.0.29"
  }
}
